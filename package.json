{
  "name": "bmad-github-native-full-cycle",
  "version": "0.1.0",
  "description": "BMAD-GitHub Native Full Cycle: Autonomous AI-driven development workflow",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ scripts/ personas/ tests/",
    "lint:fix": "eslint src/ scripts/ personas/ tests/ --fix",
    "format": "prettier --write \"src/**/*.js\" \"scripts/**/*.js\" \"personas/**/*.js\" \"tests/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"scripts/**/*.js\" \"personas/**/*.js\" \"tests/**/*.js\"",
    "setup": "./bmad/bin/setup-tools.sh",
    "bmad:gatekeeper": "node scripts/bmad/bmad-gatekeeper.js",
    "bmad:doc": "node scripts/bmad/agent-doc.js",
    "bmad:workflow": "node scripts/bmad/bmad-workflow.js",
    "bmad:test": "npm run test && npm run lint && npm run bmad:gatekeeper"
  },
  "keywords": [
    "bmad",
    "github",
    "autonomous",
    "ai",
    "development"
  ],
  "author": "helton-godoy",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@eslint/js": "^8.57.1",
    "babel-jest": "^29.7.0",
    "cors": "^2.8.5",
    "eslint": "^8.55.0",
    "express-validator": "^7.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.4",
    "winston": "^3.11.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
